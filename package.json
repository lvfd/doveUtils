{
  "name": "dove-utils",
  "version": "2.0.0",
  "description": "Compile, compress and publish the javascript for all projects.",
  "main": "index.js",
  "scripts": {
    "build-ban": "npx webpack --config-name ban",
    "build-dovemgr": "npx webpack --config webpack.dovemgr.js",
    "build-dovepay": "npx webpack --config webpack.dovepay.js",
    "fredev": "npx webpack --config-name dovepay-freight --node-env development",
    "frepro": "npx webpack --config-name dovepay-freight --node-env production",
    "test": "npx webpack --config-name test",
    "prod-dovemgr": "npm run build-ban && npm run build-dovemgr",
    "prod-dovepay": "npm run build-ban && npm run build-dovepay"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvfd/doveUtils.git"
  },
  "keywords": [
    "javascript",
    "webpack",
    "express"
  ],
  "author": "lvfd@acca.com.cn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lvfd/doveUtils/issues"
  },
  "homepage": "https://github.com/lvfd/doveUtils#readme",
  "browserslist": [
    "IE >= 9"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint-plugin-json": "^3.1.0",
    "file-loader": "^6.2.0",
    "jquery-lts": "npm:jquery@^3.6.1",
    "jquery-v2": "npm:jquery@^2.2.4",
    "showmodaldialog": "^1.0.0",
    "style-loader": "^3.3.1",
    "uikit-lts": "github:lvfd/uikit#master",
    "uikit-v2": "npm:uikit@^2.27.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "browser-dtector": "^3.2.0",
    "chart.js": "^3.9.1",
    "core-js": "^3.26.0",
    "dom4": "^2.1.6"
  }
}
